
<table class="table span4">

  <thead>
    <th>Student</th>
    <th>Sheet</th>
  </thead>

  [% FOR user IN users %]
  <tr>
    <td>
      <a href="[% request.uri_base %]/users/[% user.id %]">[% user.name %]</a>
    </td>
    <td> [% user.last_sheet %] </td>
    <td>
      <a href="[% request.uri_base %]/users/[% user.id %]/report">view progress</a>
    </td>
  </tr>
  [% END %]

</table>

